title={Variation in rates of ICU readmissions and post-ICU in-hospital mortality and their association with ICU discharge practices},
journal={BMC Health Services Research},
year={2017},
volume={17},
number={1},
doi={10.1186/s12913-017-2234-z},
art_number={281},
