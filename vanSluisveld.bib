@ARTICLE{vanSluisveld201789,
author={van Sluisveld, M.A.E. and Hof, A.F. and van Vuuren, D.P. and Boot, P. and Criqui, P. and Matthes, F.C. and Notenboom, J. and Pedersen, S.L. and Pfluger, B. and Watson, J.},
